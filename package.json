{
  "name": "eywa-chat",
  "version": "1.0.0",
  "description": "Persistent-memory chatbot with Next.js, SeekDB, and LangGraph",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "predev": "node scripts/validate-env.mjs",
    "dev": "next dev",
    "prebuild": "node scripts/validate-env.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "auth:jwt:generate": "node scripts/generate-jwt.mjs",
    "loadtest:chat": "node scripts/loadtest-chat.mjs",
    "loadtest:report": "node scripts/render-loadtest-report.mjs",
    "loadtest:chat:report": "node scripts/loadtest-chat-and-report.mjs",
    "loadtest:chat:report:autojwt": "node scripts/loadtest-chat-report-with-jwt.mjs",
    "loadtest:scenarios": "node scripts/loadtest-scenarios.mjs",
    "issue26:prepare": "node scripts/prepare-issue26-closure.mjs",
    "issue26:finalize": "node scripts/issue26-finalize.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kejun/eywa-chat.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kejun/eywa-chat/issues"
  },
  "homepage": "https://github.com/kejun/eywa-chat#readme",
  "dependencies": {
    "@langchain/langgraph": "^1.1.5",
    "@langchain/openai": "^1.2.8",
    "ansi-to-react": "^6.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jose": "^6.1.3",
    "lucide-react": "^0.574.0",
    "next": "^16.1.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "seekdb": "^1.1.1",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.5.6",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4.2.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
